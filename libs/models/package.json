{
  "name": "@purrfect-sitter/models",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "default": "./src/index.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {},
  "nx": {
    "tags": [
      "scope:shared",
      "type:core"
    ],
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "dist/libs/models",
          "main": "libs/models/src/index.ts",
          "tsConfig": "libs/models/tsconfig.lib.json",
          "assets": [
            "libs/models/*.md"
          ]
        }
      }
    }
  }
}
